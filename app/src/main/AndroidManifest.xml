<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.innopolis.android.androidstudents">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/students"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/new_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.ManagerActivity"></activity>
        <activity android:name=".Activities.RegistrationActivity"></activity>
        <activity android:name=".Activities.StudentDetailsActivity"></activity>
    </application>

</manifest>